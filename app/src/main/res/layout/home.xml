<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <com.google.android.material.navigation.NavigationView
        android:layout_width="match_parent"
        android:layout_height="53dp"
        android:background="#BFA2DB" />


    <ImageButton
        android:id="@+id/psychome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="190dp"
        android:layout_marginRight="98dp"
        android:background="@drawable/btnimg"
        android:elevation="20dp"
        android:padding="10dp"
        app:srcCompat="@drawable/psychbtn"

        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/diareyhome"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="500dp"
        android:layout_marginRight="98dp"
        android:background="@drawable/btnimg"
        android:elevation="20dp"
        android:padding="10dp"
        app:srcCompat="@drawable/diarybtn"

        />

    <ImageButton
        android:id="@+id/grphome"
        android:layout_width="173dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="350dp"
        android:layout_marginRight="98dp"
        android:background="@drawable/btnimg"
        android:elevation="20dp"
        android:padding="10dp"
        app:srcCompat="@drawable/grpbtn"

        />

    <TextView
        android:id="@+id/signintxt"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="110dp"
        android:layout_marginRight="98dp"
        android:text="Helping Hand"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#512D6D" />


</RelativeLayout>